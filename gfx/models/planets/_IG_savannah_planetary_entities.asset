# savannah Planet 03
entity = {
	name = "savannah_planet_03_entity"
	pdxmesh = "planet_clouded_mesh"
	meshsettings = {
		name = "planet_geosphereShape"
		texture_diffuse = "ig_savannah_03_2k_diffuse.dds"
		texture_normal = "ig_savannah_03_2k_normal.dds"
		texture_specular = "ig_savannah_03_2k_specular.dds"
	}
	##cloud texture override
	meshsettings = {
		name = "clouds_geosphereShape"
		texture_diffuse = "ig_clouds_medium_09_4k_diffuse.dds"
		texture_normal = "ig_clouds_medium_09_4k_normal.dds"
		texture_specular = "nospec.dds"
	}
	##cloud shadow texture override
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "ig_clouds_medium_09_2k_shadow_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class_02" } }
	}
}
