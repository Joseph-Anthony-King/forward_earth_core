# Toxic planet 05
entity = {
	name = "toxic_planet_05_entity"
	pdxmesh = "planet_clouded_mesh"
	meshsettings = {
		name = "planet_geosphereShape"
		texture_diffuse = "ig_toxic_05_2k_diffuse.dds"
		texture_normal = "ig_toxic_05_2k_normal.dds"
		texture_specular = "ig_toxic_05_2k_specular.dds"
	}
	##pole texture override
	meshsettings = {
		name = "polesShape"
		texture_diffuse = "ig_pole_no_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}
	##cloud texture override
	meshsettings = {
		name = "clouds_geosphereShape"
		texture_diffuse = "ig_clouds_toxic_01_4k_diffuse.dds"
		texture_normal = "ig_clouds_toxic_01_2k_normal.dds"
		texture_specular = "nospec.dds"
	}
	##cloud shadow texture override
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "ig_clouds_toxic_01_2k_shadow_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}
	default_state = "idle"
	state = { name = "idle" animation = "idle" 
		start_event = { trigger_once = yes sound = { soundeffect = "amb_toxic_world_a" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_toxic_world_b" } }
	}
}
