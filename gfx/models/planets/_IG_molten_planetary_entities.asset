# Molten Planet 04
entity = {
	name = "molten_planet_04_entity"
	pdxmesh = "planet_no_pole_no_clouds_mesh"
	meshsettings = {
		name = "planet_geosphereShape"
		shader = "PdxMeshPlanetEmissive"
		texture_diffuse = "ig_molten_04_2k_diffuse.dds"
		texture_normal = "ig_molten_04_2k_normal.dds"
		texture_specular = "ig_molten_04_2k_specular.dds"
	}
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_molten_world_a" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_molten_world_b" } }
	}
}
